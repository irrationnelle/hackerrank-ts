{
  "name": "hackerrank-ts",
  "version": "0.0.1",
  "description": "solution for problems of hackerrank",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha"
  },
  "type": "module",
  "dependencies": {
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/eslint": "^7.2.4",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.10",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "chai": "^4.2.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "mocha": "^8.2.0",
    "ts-node": "^9.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": false,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "airbnb",
      "airbnb/hooks",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": false,
        "tsx": false
      },
      "ecmaVersion": 11,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "implicit-arrow-linebreak": ["error", "below"],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "**/*.test.js",
            "**/*.spec.js",
            "**/*.test.jsx",
            "**/*.spec.jsx",
            "**/*.test.ts",
            "**/*.spec.ts",
            "**/*.test.tsx",
            "**/*.spec.tsx"
          ]
        }
      ],
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "ts": "never",
          "json": "never",
          "d.ts": "never"
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".ts"
          ]
        }
      ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".ts"
          ]
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/BARlSTAS/test-bucket.git"
  },
  "keywords": [
    "mocha",
    "tdd",
    "typescript"
  ],
  "author": "irrationnelle <drakkarverenis@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BARlSTAS/test-bucket/issues"
  },
  "homepage": "https://github.com/BARlSTAS/test-bucket#readme"
}
